public class Main {
    public static void main(String[] args) {
        int[] sayilar =new int[5];

        //sayilar dizisine elemanları atayalım
        for(int i=0;i<sayilar.length;i++){
            sayilar[i]=i*10;
        }
        System.out.println("sayilar dizisi içerigi ");
        for(int i=0;i<sayilar.length;i++){
            System.out.println(sayilar[i]);
        }

    }
}
