import java.util.Scanner;
public class Main {
    public static void main(String[] args){
        Scanner gir = new Scanner(System.in);
        System.out.println("Bir sayi giriniz:");
        int sayi = gir.nextInt();
        if(sayi%2==0){
            System.out.println("cift");
        }
        else{
            System.out.println("tek");
        }
    }
}
