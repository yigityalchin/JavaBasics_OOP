public class Toplam {
    public static void main(String[] args) {
        int[] numbers = {5, 10, 15, 20, 25};
        
        int toplam = 0;
        for (int number : numbers) {
            toplam += number;
        }
        
        double ortalama = (double) toplam / numbers.length;
        
        System.out.println("Toplam: " + toplam);
        System.out.println("Ortalama: " + ortalama);
    }
}
