    public static void main(String[] args){
    Scanner input = new Scanner(System.in);
    System.out.println("adet girin");
    int abc= input.nextInt();

    int[] numara = new int[abc];
    int[] tersnumara = new int[numara.length];

    for(int i = 0;i<abc;i++){
    System.out.println("sayÄ± girin ");
    int eleman =input.nextInt();
    numara[i] = eleman;}
      int j = 0;
    for(int i = numara.length-1; i>=0; i--){
            tersnumara[j] = numara[i];
            j++;
        }
        System.out.println(Arrays.toString(tersnumara));

    }
