//2311502260
// Array Sorting

import java.util.Arrays;
import java.util.Scanner;

public class Odev {
    public static void main(String[] args) {
        Scanner num = new Scanner(System.in);
        System.out.println("Eleman sayısını belirleyin:");
        int deger = num.nextInt();
        int[] dizi = new int[deger];

        for(int i = 0; i < deger; i++) {
            System.out.println("Değer giriniz:");
            dizi[i] = num.nextInt();


        }

        Arrays.sort(dizi);

        System.out.println("Sırala: " + Arrays.toString(dizi));





    }
}
