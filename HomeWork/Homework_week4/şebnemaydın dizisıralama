import java.util.Scanner;
public class Main{
    public static void main(String[] args) {
        Scanner gir= new Scanner(System.in);

        System.out.println("eleman sayisi gir:");
        int a = gir.nextInt();
        int[] dizi= new int[a];
        System.out.println("elemanlar:");
        for (int i = 0; i < a; i++) {
            dizi[i] = gir.nextInt();
        }
        for (int i = 0; i < a; i++) {
            System.out.print(dizi[i] + "\t");
        }
    }
}
