import java.util.Scanner;
public class DogrusalArama {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int dizi[] = new int[] {10,20,30,40,50};
        System.out.println("Dizide bulmak istediginiz sayiyi giriniz: ");
        int sayi = input.nextInt();
        for(int i = 0; i < dizi.length; i++)
        {
            if (dizi[i] == sayi) {
                System.out.println("Aradiginiz sayi bu dizide bulunmaktadir.");
            }
            else {
                System.out.println("Aradiginiz sayi dizide bulunmamaktadir.");
            }
        }
    }
} 

