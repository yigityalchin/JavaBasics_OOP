//Multiplication Table 

import java.util.Scanner;

public class Odev {
    public static void main(String[] args) {
        Scanner num = new Scanner(System.in);
        System.out.println("SayÄ± girin:");
        int sayi = num.nextInt();

        int carpim = 1;


        int i;
        for (i = 1; i <= 9; i++) {
            carpim = sayi * i;


            System.out.println(sayi + " * " + i + " = " + carpim);
        }

    }
}
